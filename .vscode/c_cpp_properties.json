{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/tools/sdk/rtos/freertos/portable/GCC/ARM_CM4F",
                "${workspaceFolder}/tools/sdk/devices/MIMXRT1064/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "SDK_DEBUGCONSOLE=1",
